{
  "name": "express-mongoose-otp",
  "version": "1.0.0",
  "description": "Otp validation for express using mongoose database",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ffegu/express-mongoose-otp.git"
  },
  "keywords": [
    "express",
    "mongoose",
    "otp",
    "mongoose",
    "otp",
    "otp",
    "mongoose",
    "express",
    "otp",
    "otp",
    "nodejs"
  ],
  "author": "Pran Pegu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ffegu/express-mongoose-otp/issues"
  },
  "homepage": "https://github.com/ffegu/express-mongoose-otp#readme",
  "dependencies": {
    "mongoose": "^5.12.13"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "jest": "^27.0.4",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  }
}
